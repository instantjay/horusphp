{
    "name": "instantjay/horusphp",
    "description": "A collection of libraries for detecting bots/humans and quickly validating data.",
    "license": "MIT",
    "autoload": {
        "psr-4": {"instantjay\\horusphp\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"instantjay\\horusphp\\Tests\\": "tests"}
    },
    "require": {
        "gregwar/captcha": "1.*",
        "egulias/email-validator": "2.*",
        "respect/validation": "1.*",
        "jenssegers/agent": "2.*",
        "cocur/slugify": "3.*",
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^1.2"
    }
}
